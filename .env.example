# Environment Configuration for StudyPartner Integration

# Copy this file to .env.local and configure your StudyPartner API settings

# StudyPartner API Configuration
VITE_STUDYPARTNER_API_URL=https://api.studypartner.app/v1
VITE_STUDYPARTNER_API_KEY=your_api_key_here

# For local development, you might use:
# VITE_STUDYPARTNER_API_URL=http://localhost:3001/api/v1

# Google OAuth Configuration (for Google Sign-In)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Development Settings
VITE_ENVIRONMENT=production
VITE_DEBUG_SYNC=false

# Enable/disable features
VITE_ENABLE_STUDYPARTNER_SYNC=true
VITE_ENABLE_OFFLINE_MODE=true