# Default environment variables for development
# Copy this to .env.local and replace with your actual Firebase credentials

# Firebase Configuration (Replace with actual values from Firebase Console)
VITE_FIREBASE_API_KEY=demo-api-key
VITE_FIREBASE_AUTH_DOMAIN=demo-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=demo-project
VITE_FIREBASE_STORAGE_BUCKET=demo-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:demo-app-id

# Application features
VITE_ENABLE_FIREBASE_AUTH=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_DEBUG_FIREBASE=false